<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.student.homemade">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpSeller"></activity>
        <activity
            android:name=".SignUpBuyer"
            android:label="@string/title_activity_sign_up_buyer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StartPage" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="Homemade" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RatingandReviewActivity"
            android:label="Ratings and Reviews"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".CancelOrder"
            android:label="Cancel Order"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".BreakfastActivity"
            android:label="Breakfast"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".LunchActivity"
            android:label="Lunch"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".DinnerActivity"
            android:label="Lunch"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".SnacksActivity"
            android:label="Lunch"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".ChooseActivity"
            android:label="Lunch"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".OrdersHistoryActivity"
            android:label="Lunch"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".CurrentOrdersActivity"
            android:label="Lunch"
            android:theme="@style/AppTheme"
            />

    </application>

</manifest>